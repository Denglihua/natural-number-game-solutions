induction b with k h,
rw mul_zero (succ a),
rw mul_zero a,
rw add_zero 0,
refl,

rw add_succ (a * succ k) k,
rw mul_succ a k,
rw succ_eq_add_one k,
rw mul_add (succ a) k 1,
rw h,
rw mul_one (succ a),
rw add_assoc (a * k) k (succ a),
rw add_succ k a,
rw add_comm k a,
rw add_succ (a * k) (a + k),
rw add_assoc (a * k) a k,
refl,
