rw two_eq_succ_one,
rw one_eq_succ_zero,
repeat {rw pow_succ},
rw pow_zero (a + b),
rw pow_zero a,
rw pow_zero b,
rw one_mul (a + b),
rw one_mul a,
rw one_mul b,
rw add_mul a b (a + b),
rw mul_add a a b,
rw mul_add b a b,
rw succ_mul (succ 0) a,
rw succ_mul 0 a,
rw zero_mul a,
rw
zero_add a,
rw add_mul a a b,
rw mul_comm b a,
rw add_comm (a * b) (b * b),
simp,
